# PriceSentinel Environment Configuration Template
# Copy this file to .env and fill in your actual values

# ENTSO-E Transparency Platform API Key
# Register at: https://transparency.entsoe.eu/
ENTSOE_API_KEY=your_entsoe_api_key_here

# Open-Meteo API (free tier, no key required)
# OPEN_METEO_API_KEY=

# Logging Configuration
LOG_LEVEL=INFO
LOG_DIR=logs

# Data Directory (optional, defaults to './data')
# DATA_DIR=data

# Model Directory (optional, defaults to './models')
# MODEL_DIR=models
